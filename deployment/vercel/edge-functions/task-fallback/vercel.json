{
  "functions": {
    "api/tasks/[...path].js": {
      "runtime": "edge"
    },
    "api/webhooks/[...path].js": {
      "runtime": "edge"
    },
    "api/health.js": {
      "runtime": "edge"
    }
  },
  "rewrites": [
    {
      "source": "/tasks/(.*)",
      "destination": "/api/tasks/$1"
    },
    {
      "source": "/webhooks/(.*)",
      "destination": "/api/webhooks/$1"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization, X-API-Key"
        },
        {
          "key": "X-Served-By",
          "value": "vercel-edge-fallback"
        }
      ]
    }
  ]
}